{
  "name": "psffpp",
  "version": "1.0.0",
  "description": "PS010 PSF File Pinning Protocol",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint && TEST=unit c8 --reporter=text mocha test/unit/",
    "test:integration": "mocha --timeout 120000 test/integration/",
    "lint": "standard --env mocha --fix",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs",
    "coverage:report": "c8 --reporter=html mocha test/unit/ --exit"
  },
  "keywords": [
    "bitcoin",
    "bitcoin cash",
    "wallet",
    "javascript",
    "cryptocurrency",
    "react",
    "front end",
    "client",
    "apidoc",
    "slp",
    "tokens"
  ],
  "author": "Chris Troutner <chris.troutner@gmail.com>",
  "license": "MIT",
  "apidoc": {
    "title": "psffpp",
    "url": "localhost:5000"
  },
  "repository": "Permissionless-Software-Foundation/psffpp",
  "dependencies": {
    "axios": "1.3.5",
    "psf-multisig-approval": "2.0.7"
  },
  "devDependencies": {
    "@psf/bch-js": "6.8.0",
    "apidoc": "0.54.0",
    "c8": "9.1.0",
    "chai": "4.3.7",
    "husky": "8.0.3",
    "lodash.clonedeep": "4.5.0",
    "minimal-slp-wallet": "5.12.0",
    "mocha": "10.2.0",
    "semantic-release": "19.0.5",
    "sinon": "15.0.3",
    "standard": "17.0.0"
  },
  "release": {
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": true
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
